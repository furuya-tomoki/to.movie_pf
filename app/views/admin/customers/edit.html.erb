<h2>会員情報編集</h2>

<%= form_with model: @customer, url: {controller: 'admin/customers', action: 'update' }, local:true do |f| %>



<%= f.label :"名前" %>
<%= f.text_field :name %>


<%= f.label :image %>
<%= f.attachment_field :image, class: "customer_image" %>

<%= f.submit "保存する", class:"btn btn-success" %>

<% end %>